  "version": "0.2.0",
  "configurations": [
    {
      "name": "Detect COCO",
      "type": "python",
      "request": "launch",
      "program": "detect.py",
      "args": [
        "--weights",
        "${workspaceFolder}/checkpoints/yolov3_ckpt_60.pth",
        "--image_folder",
        ".\\input\\"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Train Yolo",
      "type": "python",
      "request": "launch",
      "program": "train.py",
      "console": "integratedTerminal",
      "args": [
        "--data_config",
        "config/coco.data"
      ]
    },
    {
      "name": "Train Urban",
      "type": "python",
      "request": "launch",
      "program": "train.py",
      "console": "integratedTerminal",
      "args": [
        "--data_config",
        "config/urbanrecall.data",
        "--model_def",
        "config/yolov3_3d.cfg"
      ]
    }
  ]
}